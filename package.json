{
  "name": "sylvan-color-theme",
  "displayName": "Sylvan Color Theme",
  "description": "a new color theme",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Sylvan",
        "uiTheme": "vs-dark",
        "path": "./themes/sylvan-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "node src/index.js",
    "build:watch": "onchange -i src/**/*.js -- npm run build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1"
  }
}
